curl http://127.0.0.1:5000/?board=oo+xx++++
curl http://127.0.0.1:5000/?board=+ooxx++++
curl http://127.0.0.1:5000/?board=++o+ox+x+
curl http://127.0.0.1:5000/?board=+++oo+xx+
curl http://127.0.0.1:5000/?board=++++ooxx+
curl http://127.0.0.1:5000/?board=+++++oxxo
curl http://127.0.0.1:5000/?board=++++xxoo+
curl http://127.0.0.1:5000/?board=++++xx+oo
curl http://127.0.0.1:5000/?board=ox+ooxxx+
# BLOCK strategy
curl http://127.0.0.1:5000/?board=xx+o++o++
curl http://127.0.0.1:5000/?board=x+++++oox
curl http://127.0.0.1:5000/?board=+xx++o+o+
curl http://127.0.0.1:5000/?board=++x+++oox
curl http://127.0.0.1:5000/?board=+++xx+o+o
curl http://127.0.0.1:5000/?board=+o++xx+o+
curl http://127.0.0.1:5000/?board=+o+++x++x
curl http://127.0.0.1:5000/?board=o+++++xx+
# FORK strategy
curl http://127.0.0.1:5000/?board=o+++x+x+o
curl http://127.0.0.1:5000/?board=+o++xox++
curl http://127.0.0.1:5000/?board=+xo+x+o++
curl http://127.0.0.1:5000/?board=++xox++o+
curl http://127.0.0.1:5000/?board=x+++xo+o+
# BLOCK A FORK strategy
curl http://127.0.0.1:5000/?board=x+++o+++x
curl http://127.0.0.1:5000/?board=x+++o+++x
curl http://127.0.0.1:5000/?board=++x+o+x++
curl http://127.0.0.1:5000/?board=++x+x+o++
# CENTER strategy
curl http://127.0.0.1:5000/?board=+++++++++
# OPPOSITE CORNER strategy
curl http://127.0.0.1:5000/?board=x++++++++
curl http://127.0.0.1:5000/?board=++x++++++
curl http://127.0.0.1:5000/?board=++++++x++
curl http://127.0.0.1:5000/?board=++++++++x
# EMPTY CORNER strategy
curl http://127.0.0.1:5000/?board=+x+oox+x+
curl http://127.0.0.1:5000/?board=x+++o++++
# EMPTY SIDE strategy
curl http://127.0.0.1:5000/?board=o+xxoooxx
