# Check for board errors
curl http://127.0.0.1:5000/?board=x
curl http://127.0.0.1:5000/?board=badchar
curl http://127.0.0.1:5000/?board=oooxxx+++
curl http://127.0.0.1:5000/?board=ooo+++xxx
curl http://127.0.0.1:5000/?board=xxxooo+++
curl http://127.0.0.1:5000/?board=+++oooxxx
curl http://127.0.0.1:5000/?board=xxx+++ooo
curl http://127.0.0.1:5000/?board=+++xxxooo
curl http://127.0.0.1:5000/?board=ox+ox+ox+
curl http://127.0.0.1:5000/?board=o+xo+xo+x
curl http://127.0.0.1:5000/?board=xo+xo+xo+
curl http://127.0.0.1:5000/?board=+ox+ox+ox
curl http://127.0.0.1:5000/?board=x+ox+ox+o
curl http://127.0.0.1:5000/?board=+xo+xo+xo
# VICTORY/DEFEAT boards
curl http://127.0.0.1:5000/?board=oooxx++++
curl http://127.0.0.1:5000/?board=xx+ooo+++
curl http://127.0.0.1:5000/?board=ox+ox+o++
